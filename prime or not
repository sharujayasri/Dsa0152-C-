#include<iostream>
using namespace std;
int main()
{
	int num;
	cout<<"Enter the number:";
	cin>>num;
	if(num<=1)
	{
		cout<<"its not a prime number";
		return 0;
	}
	bool isprime =true; 
	for(int i=2;i<num;i++)
	{
		if(num%i==0)
		{
			isprime=false;
		
		break;
	}
}
	if (isprime)
	{
		cout<<"is prime";
	}
	else
	{
		cout<<"not a prime";
	}
}  
